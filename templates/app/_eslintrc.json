{
    "env": {
        "node": true,
        "browser": true
    },
    "ecmaFeatures": {
        "modules": true,
        "impliedStrict": true
    },
    "rules": {
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-catch-shadow": 2,
        "no-class-assign": 2,
        "no-cond-assign": [2, "always"],
        "no-confusing-arrow": 2,
        "no-console": 0,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-continue": 0,
        "no-control-regex": 2,
        "no-debugger": 1,
        "no-delete-var": 2,
        "no-div-regex": 0,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-empty": 1,
        "no-empty-character-class": 2,
        "no-empty-function": 1,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 0,
        "no-extra-bind": 0,
        "no-extra-boolean-cast": 2,
        "no-extra-label": 0,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implicit-coercion": 2,
        "no-implicit-globals": 1,
        "no-implied-eval": 2,
        "no-inline-comments": 1,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-invalid-this": 2,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 0,
        "no-lone-blocks": 1,
        "no-lonely-if": 0,
        "no-loop-func": 0,
        "no-mixed-requires": 0,
        "no-mixed-spaces-and-tabs": 2,
        "linebreak-style": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-multiple-empty-lines": [1, {"max": 2, "maxBOF": 1, "maxEOF": 1}],
        "no-native-reassign": 1,
        "no-negated-condition": 0,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-require": 2,
        "no-new-symbol": 2,
        "no-new-wrappers": 1,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 0,
        "no-param-reassign": 0,
        "no-path-concat": 2,
        "no-plusplus": 1,
        "no-process-env": 2,
        "no-process-exit": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-restricted-globals": 0,
        "no-restricted-imports": 0,
        "no-restricted-modules": 0,
        "no-restricted-syntax": 0,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 1,
        "no-shadow-restricted-names": 2,
        "no-whitespace-before-property": 1,
        "no-spaced-func": 1,
        "no-sparse-arrays": 2,
        "no-sync": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-this-before-super": 2,
        "no-throw-literal": 1,
        "no-undef": 2,
        "no-undef-init": 1,
        "no-undefined": 2,
        "no-unexpected-multiline": 2,
        "no-underscore-dangle": 0,
        "no-unmodified-loop-condition": 0,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 1,
        "no-unused-labels": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-useless-call": 1,
        "no-useless-concat": 2,
        "no-useless-constructor": 2,
        "no-void": 0,
        "no-var": 0,
        "no-warning-comments": [1, { "terms": ["todo", "fixme"], "location": "start" }],
        "no-with": 2,
        "no-magic-numbers": [1, {"ignoreArrayIndexes": true }],
        "array-bracket-spacing": [1, "never"],
        "array-callback-return": 1,
        "arrow-body-style": 2,
        "arrow-parens": 1,
        "arrow-spacing": 1,
        "accessor-pairs": 1,
        "block-scoped-var": 1,
        "block-spacing": [1, "always"],
        "brace-style": [1, "1tbs", { "allowSingleLine": true }],
        "callback-return": 0,
        "camelcase": [1, {"properties": "never"}],
        "comma-dangle": 2,
        "comma-spacing": 1,
        "comma-style": [1, "last"],
        "complexity": [0, 11],
        "computed-property-spacing": 1,
        "consistent-return": 0,
        "consistent-this": 0,
        "constructor-super": 2,
        "curly": 1,
        "default-case": 0,
        "dot-location": 1,
        "dot-notation": 1,
        "eol-last": 1,
        "eqeqeq": 1,
        "func-names": 0,
        "func-style": 0,
        "generator-star-spacing": 0,
        "global-require": 0,
        "guard-for-in": 0,
        "handle-callback-err": 1,
        "id-length": [0, {"min": 2, "max": 20}],
        "indent": [1, 2],
        "init-declarations": 0,
        "jsx-quotes": 0,
        "key-spacing": 1,
        "keyword-spacing": 1,
        "lines-around-comment": 1,
        "max-depth": [1, {"maximum": 10}],
        "max-len": [1, {"code": 100, "tabWidth": 2, "ignoreUrls": true}],
        "max-nested-callbacks": [1, {"maximum": 5}],
        "max-params": 0,
        "max-statements": 0,
        "new-cap": 1,
        "new-parens": 1,
        "newline-after-var": 1,
        "newline-before-return": 2,
        "newline-per-chained-call": [1, {"ignoreChainWithDepth": 8}],
        "object-curly-spacing": [1, "never"],
        "object-shorthand": 0,
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "operator-assignment": [1, "never"],
        "operator-linebreak": 1,
        "padded-blocks": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-reflect": 0,
        "prefer-rest-params": 1,
        "prefer-spread": 0,
        "prefer-template": 0,
        "quote-props": [2, "as-needed"],
        "quotes": [1, "single", "avoid-escape"],
        "radix": 2,
        "id-match": 0,
        "id-blacklist": 0,
        "require-jsdoc": 0,
        "require-yield": 0,
        "semi": 2,
        "semi-spacing": 1,
        "sort-vars": 0,
        "sort-imports": 1,
        "space-before-blocks": 1,
        "space-before-function-paren": [1, { "anonymous": "always", "named": "never" }],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": 1,
        "strict": 1,
        "template-curly-spacing": 0,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "vars-on-top": 1,
        "wrap-iife": [1, "any"],
        "wrap-regex": 1,
        "yield-star-spacing": 0,
        "yoda": 1
    }
}